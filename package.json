{
  "name": "roche-fte-assessment",
  "version": "1.0.0",
  "description": "Roche [Frontend Technical Expert] assessment",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "clean:dist": "rimraf dist",
    "start": "npm-run-all --parallel start:mock start:dev typecheck:watch",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack/webpack.dev.js",
    "start:mock": "json-server --watch ./mock/db.json",
    "build": "npm run clean && cross-env NODE_ENV=development webpack --config ./config/webpack/webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production BASE_URL=http://localhost:1338 webpack --config ./config/webpack/webpack.prod.js --optimize-minimize",
    "build:dist": "npm-run-all clean:dist build:prod build:copyfiles",
    "build:copyfiles": "copyfiles package.json README.md LICENSE.txt dist",
    "typecheck": "tsc --pretty --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "cross-env NODE_ENV=test jest --verbose --config ./config/jest/jest.config.js --passWithNoTests --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --verbose --watchAll --config ./config/jest/jest.config.js --no-cache"
  },
  "keywords": [],
  "author": "Óscar Garcia Masiá",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "fontsource-roboto": "^3.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "copyfiles": "^2.3.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "json-server": "^0.16.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
